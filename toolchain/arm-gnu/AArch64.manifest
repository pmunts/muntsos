# Component data for gcc
gcc_url=https://developer.arm.com/-/media/Files/downloads/gnu/12.3.rel1/src
gcc_filespec=gcc.tar.xz
gcc_makeflags="MAKEINFOFLAGS=--force"
gcc_mingw_only="no"
gcc_linuxhost_only="no"
gcc_configure=
gcc_mingw_extraconf="--with-libiconv-prefix=${local_builds}/destdir/${host}/usr --with-system-zlib=no"
gcc_staticlink="no"
gcc_stage1_flags="--with-mpc=${local_builds}/destdir/${host} --with-mpfr=${local_builds}/destdir/${host} --with-gmp=${local_builds}/destdir/${host} --without-headers --with-newlib --without-cloog --without-isl --disable-libatomic --disable-libsanitizer --disable-libssp --disable-libgomp --disable-libmudflap --disable-libquadmath --enable-checking=release --enable-languages=c,c++ --with-sysroot=${local_builds}/sysroot-aarch64-muntsos-linux-gnu --disable-multilib --disable-libstdcxx --disable-shared --disable-threads --disable-libvtv --enable-fix-cortex-a53-843419"
gcc_stage2_flags="--with-mpc=${local_builds}/destdir/${host} --with-mpfr=${local_builds}/destdir/${host} --with-gmp=${local_builds}/destdir/${host} --with-build-sysroot=${sysroots} --with-sysroot=${local_builds}/destdir/${host}/aarch64-muntsos-linux-gnu/libc --enable-gnu-indirect-function --enable-shared --disable-libssp --disable-libmudflap --enable-checking=release --enable-languages=ada,c,c++,fortran,go --enable-fix-cortex-a53-843419"

# Component data for binutils
binutils_url=git://sourceware.org/git
